{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "globals": {
        "$": false,
        "chrome": false,
        "loadSettings": true
    },
    "rules":{
        "no-plusplus": "off",
        "no-use-before-define": "off",
        "no-unused-vars": "warn",
        "prefer-const": "warn",
        "max-len": "off",
        "consistent-return": "off",
        "prefer-destructuring": "off",
        "func-names": "off",
        "no-console": "off",
        "no-param-reassign": ["error", { "props": false }]
    }
}